; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="Jam2023"
run/main_scene="res://game/scenes/Main Menu.tscn"
config/icon="res://game/assets/sprites/icon.png"

[editor_plugins]

enabled=PoolStringArray(  )

[global]

color=false

[gui]

common/drop_mouse_on_gui_input_disabled=true
theme/use_hidpi=true

[mono]

project/assembly_name="Jam2023"

[network]

limits/debugger_stdout/max_chars_per_second=500000
limits/debugger_stdout/max_messages_per_frame=10000
limits/debugger_stdout/max_errors_per_second=10000
limits/debugger_stdout/max_warnings_per_second=10000

[physics]

common/enable_pause_aware_picking=true
2d/physics_engine="GodotPhysics"

[rendering]

quality/driver/driver_name="GLES2"
environment/default_clear_color=Color( 0.0156863, 0.0156863, 0.901961, 1 )
environment/default_environment="res://default_env.tres"

[tiled_importer]

enable_json_format=true
